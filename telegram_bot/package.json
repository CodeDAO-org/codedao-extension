{
  "name": "codedao-telegram-bot",
  "version": "1.0.0",
  "description": "CodeDAO Telegram Bot - Earn cryptocurrency for coding!",
  "main": "bot.js",
  "scripts": {
    "start": "node bot.js",
    "dev": "nodemon bot.js",
    "test": "jest",
    "docker:build": "docker build -t codedao-telegram-bot .",
    "docker:run": "docker run -d --env-file .env codedao-telegram-bot",
    "deploy:github": "node github-deploy.js",
    "bot:run": "node github-bot-runner.js",
    "bot:test": "node github-bot-runner.js --test",
    "transparency": "echo \"View source: https://github.com/CodeDAO-org/codedao-extension\""
  },
  "keywords": [
    "telegram",
    "bot",
    "cryptocurrency",
    "coding",
    "rewards",
    "web3",
    "base-network"
  ],
  "author": "CodeDAO Team",
  "license": "MIT",
  "dependencies": {
    "node-telegram-bot-api": "^0.64.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "ethers": "^6.15.0",
    "axios": "^1.5.0",
    "dotenv": "^16.3.1",
    "node-cron": "^3.0.2",
    "pg": "^8.11.3",
    "redis": "^4.6.7",
    "winston": "^3.10.0",
    "express-rate-limit": "^6.10.0",
    "express-validator": "^7.0.1",
    "mongoose": "^7.5.0",
    "chart.js": "^4.4.0",
    "canvas": "^2.11.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.6.2",
    "@types/node": "^20.5.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CodeDAO-org/codedao-extension.git"
  },
  "homepage": "https://codedao-org.github.io/codedao-extension/telegram-bot/"
}