# CodeDAO GitHub Webhook Environment Variables

# GitHub App Configuration (REQUIRED)
GITHUB_WEBHOOK_SECRET=your_webhook_secret_here
GITHUB_APP_ID=your_app_id_here
GITHUB_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\nyour_private_key_here\n-----END PRIVATE KEY-----"

# Repository Allowlist (comma-separated)
ALLOWLIST_REPOS=codedao-org/codedao-extension,your-org/your-repo

# Blockchain Configuration
BASE_RPC=https://mainnet.base.org
BASE_RPC_FALLBACK=https://base-mainnet.public.blastapi.io

# EAS Configuration (OPTIONAL for MVP)
EAS_PRIVATE_KEY=0x...
EAS_SCHEMA_ID=0x...

# Application Configuration
NODE_ENV=production
PORT=3000

# Logging
LOG_LEVEL=info 