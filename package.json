{
  "name": "codedao-extension",
  "version": "1.0.0",
  "description": "CodeDAO Extension for AI Agent Coding Rewards",
  "main": "extension.js",
  "scripts": {
    "test": "hardhat test",
    "compile": "hardhat compile",
    "deploy": "hardhat run scripts/deploy-token.js",
    "deploy:base": "hardhat run scripts/deploy-token.js --network base",
    "deploy:base-sepolia": "hardhat run scripts/deploy-token.js --network base-sepolia",
    "deploy:v2:sepolia": "hardhat run scripts/deploy-v2-sepolia.js --network baseSepolia",
    "verify": "hardhat verify --network base",
    "verify:sepolia": "echo 'Run: npx hardhat verify --network baseSepolia <ADDRESS> <ARGS>'",
    "test:sepolia": "hardhat run scripts/smoke-test-sepolia.js --network baseSepolia",
    "clean": "hardhat clean",
    "start": "node server.js",
    "dev": "nodemon server.js"
  },
  "keywords": [
    "blockchain",
    "ethereum",
    "base",
    "coding-rewards",
    "ai-agents",
    "web3",
    "vscode-extension"
  ],
  "author": "CodeDAO Team",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^4.0.0",
    "@nomicfoundation/hardhat-verify": "^2.1.1",
    "@openzeppelin/contracts": "^5.4.0",
    "dotenv": "^17.2.1",
    "ethers": "^6.15.0",
    "hardhat": "^2.26.2",
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
