{
  "name": "codedao",
  "displayName": "CodeDAO - Earn Tokens for Coding",
  "description": "AI-powered coding assistant that rewards you with tokens for writing code",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "codedao.showStats",
        "title": "Show CodeDAO Stats"
      },
      {
        "command": "codedao.connectWallet",
        "title": "Connect Wallet"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "codedao.showStats"
        },
        {
          "command": "codedao.connectWallet"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "ethers": "^6.8.0"
  }
}
