{
  "name": "codedao-twitter-bot",
  "version": "1.0.0",
  "description": "Intelligent Twitter bot for CodeDAO developer community engagement",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "jest",
    "deploy": "vercel --prod",
    "lint": "eslint src/",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/",
    "test:health": "node src/index.js health",
    "test:content": "node src/index.js post daily_stats",
    "test:engagement": "node src/index.js engage",
    "test:integration": "npm run test:health && npm run test:content"
  },
  "keywords": [
    "twitter",
    "bot",
    "codedao",
    "web3",
    "developer",
    "automation"
  ],
  "author": "CodeDAO Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "moment": "^2.29.4",
    "mongodb": "^6.2.0",
    "node-cron": "^3.0.3",
    "openai": "^4.20.1",
    "twitter-api-v2": "^1.24.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/node": "^20.9.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.1.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
